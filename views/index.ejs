    <%- include('partials/navbar.ejs') %>
        <div id="body-wrapper">
            <div class="body-container">

            </div>

            <div class="body-container">
                <div id="fn1" class="floating-nav-element">
                    <p class="floating-nav-text">portraits</p>
                </div>
                <div id="fn2" class="floating-nav-element">
